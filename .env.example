# OpenRouter API Key (get from https://openrouter.ai)
OPENAI_KEY=your-openrouter-api-key-here

# SendGrid API Key (get from https://sendgrid.com)
SENDGRID_KEY=your-sendgrid-api-key-here

# Verified sender email address in SendGrid
FROM_EMAIL=your-verified-email@example.com

# Port for local development (optional, defaults to 3005)
PORT=3005
